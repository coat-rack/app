{
  "name": "@coat-rack/core",
  "description": "Core library reserved use within the platform. Should not depend on any other internal libraries",
  "version": "0.0.1-alpha.6",
  "private": true,
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "rxjs": "^7.8.1",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.22.4"
  },
  "devDependencies": {
    "@coat-rack/build-utils": "workspace:*",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "tsup": "^8.4.0",
    "typescript": "^5.3.3"
  },
  "exports": {
    "./async": {
      "require": {
        "types": "./dist/async.d.ts",
        "default": "./dist/async.js"
      },
      "import": {
        "types": "./dist/async.d.mts",
        "default": "./dist/async.mjs"
      }
    },
    "./event": {
      "require": {
        "types": "./dist/event.d.ts",
        "default": "./dist/event.js"
      },
      "import": {
        "types": "./dist/event.d.mts",
        "default": "./dist/event.mjs"
      }
    },
    "./iframe": {
      "require": {
        "types": "./dist/iframe.d.ts",
        "default": "./dist/iframe.js"
      },
      "import": {
        "types": "./dist/iframe.d.mts",
        "default": "./dist/iframe.mjs"
      }
    },
    "./messaging": {
      "require": {
        "types": "./dist/messaging.d.ts",
        "default": "./dist/messaging.js"
      },
      "import": {
        "types": "./dist/messaging.d.mts",
        "default": "./dist/messaging.mjs"
      }
    },
    "./models": {
      "require": {
        "types": "./dist/models.d.ts",
        "default": "./dist/models.js"
      },
      "import": {
        "types": "./dist/models.d.mts",
        "default": "./dist/models.mjs"
      }
    },
    "./rpc": {
      "require": {
        "types": "./dist/rpc.d.ts",
        "default": "./dist/rpc.js"
      },
      "import": {
        "types": "./dist/rpc.d.mts",
        "default": "./dist/rpc.mjs"
      }
    }
  }
}